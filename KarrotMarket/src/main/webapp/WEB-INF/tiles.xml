<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configureation 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<!-- tiles는 모든 것이 이 파일 안에서 구현된다. -->
<tiles-definitions>

	
	<definition name="index" template="/WEB-INF/views/layout/customer.jsp">
		<put-attribute name="asset_css" value="/inc/asset_css.jsp"></put-attribute>
		<put-attribute name="asset_javascript" value="/inc/asset_javascript.jsp"></put-attribute>
		<put-attribute name="header" value="/WEB-INF/views/inc/header.jsp"></put-attribute>
		<put-attribute name="footer" value="/WEB-INF/views/inc/footer.jsp"></put-attribute>
		<put-attribute name="content" value="/WEB-INF/views/indexcontent.jsp"></put-attribute>
	</definition>
	
	<definition name="kakaomap" template="/WEB-INF/views/kakao.jsp">
		<put-attribute name="asset_css" value="/inc/asset_css.jsp"></put-attribute>
		<put-attribute name="asset_javascript" value="/inc/asset_javascript.jsp"></put-attribute>
		<put-attribute name="content" value="/WEB-INF/views/kakaomap.jsp"></put-attribute>
	</definition>
	
	
	
	
	<definition name="member.*" template="/WEB-INF/views/layout/customer.jsp">
		<put-attribute name="asset_css" value="/inc/asset_css.jsp"></put-attribute>
		<put-attribute name="asset_javascript" value="/inc/asset_javascript.jsp"></put-attribute>
		<put-attribute name="header" value="/WEB-INF/views/inc/header.jsp"></put-attribute>
		<put-attribute name="footer" value="/WEB-INF/views/inc/footer.jsp"></put-attribute>
		<put-attribute name="content" value="/WEB-INF/views/member/{1}.jsp"></put-attribute>
	</definition>
	
	<definition name="class.*" template="/WEB-INF/views/layout/customer.jsp">
		<put-attribute name="asset_css" value="/inc/asset_css.jsp"></put-attribute>
		<put-attribute name="asset_javascript" value="/inc/asset_javascript.jsp"></put-attribute>
		<put-attribute name="header" value="/WEB-INF/views/inc/header.jsp"></put-attribute>
		<put-attribute name="footer" value="/WEB-INF/views/inc/footer.jsp"></put-attribute>
		<put-attribute name="content" value="/WEB-INF/views/class/{1}.jsp"></put-attribute>
	</definition>
	
	<definition name="admin.*" template="/WEB-INF/views/layout/admin.jsp">
		<put-attribute name="asset_css" value="/inc/admin_asset_css.jsp"></put-attribute>
		<put-attribute name="asset_javascript" value="/inc/admin_asset_javascript.jsp"></put-attribute>
		<put-attribute name="header" value="/WEB-INF/views/inc/admin_header.jsp"></put-attribute>
		<put-attribute name="footer" value="/WEB-INF/views/inc/footer.jsp"></put-attribute>
		<put-attribute name="content" value="/WEB-INF/views/admin/{1}.jsp"></put-attribute>
	</definition>
	
	<definition name="auth.*" template="/WEB-INF/views/layout/auth.jsp">
		<put-attribute name="asset_css" value="/inc/asset_css.jsp"></put-attribute>
		<put-attribute name="asset_javascript" value="/inc/asset_javascript.jsp"></put-attribute>
		<put-attribute name="header" value="/WEB-INF/views/inc/header.jsp"></put-attribute>
		<put-attribute name="footer" value="/WEB-INF/views/inc/footer.jsp"></put-attribute>
		<put-attribute name="content" value="/WEB-INF/views/auth/{1}.jsp"></put-attribute>
	</definition>
	
	
	<definition name="host.*" template="/WEB-INF/views/layout/host.jsp">
		<put-attribute name="asset_css" value="/inc/asset_css.jsp"></put-attribute>
		<put-attribute name="asset_javascript" value="/inc/asset_javascript.jsp"></put-attribute>
		<put-attribute name="header" value="/WEB-INF/views/inc/host_header.jsp"></put-attribute>
		<put-attribute name="content" value="/WEB-INF/views/host/{1}.jsp"></put-attribute>
	</definition>
	

</tiles-definitions>






































